				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] >= PN2[0] && N2[t] <= PN2[1] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] >= PN4[0] && N4[t] <= PN4[1]) pid[6].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 6;
				else if (N1[t] >= PN1[0] && N1[t] <= PN1[1] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] >= PN4[0] && N4[t] <= PN4[1]) pid[7].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 7;
				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] >= PN4[0] && N4[t] <= PN4[1]) pid[8].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 8;

				if (N1[t] >= PN1[0] && N1[t] <= PN1[1] && N2[t] >= PN2[0] && N2[t] <= PN2[1] && N3[t] >= PN3[0] && N3[t] <= PN3[1] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[9].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 9;
				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] >= PN2[0] && N2[t] <= PN2[1] && N3[t] >= PN3[0] && N3[t] <= PN3[1] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[10].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 10;
				else if (N1[t] >= PN1[0] && N1[t] <= PN1[1] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] >= PN3[0] && N3[t] <= PN3[1] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[11].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 11;
				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] >= PN3[0] && N3[t] <= PN3[1] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[12].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 12;

				else if (N1[t] >= PN1[0] && N1[t] <= PN1[1] && N2[t] >= PN2[0] && N2[t] <= PN2[1] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[13].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 13;
				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] >= PN2[0] && N2[t] <= PN2[1] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[14].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 14;
				else if (N1[t] >= PN1[0] && N1[t] <= PN1[1] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[15].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 15;
				else if (N1[t] > PN1[1] && N1[t] <= PN1[2] && N2[t] > PN2[1] && N2[t] <= PN2[2] && N3[t] > PN3[1] && N3[t] <= PN3[2] && N4[t] > PN4[1] && N4[t] <= PN4[2]) pid[16].push_back(make_pair(t + 1, T[t + 1])), B[T[t + 1]] = 16;

			}


		/*	for (int i = 1; i <= 8; i++)
			{
				cout << "pid" << i << ": ";

				for (int j = 0; j < pid[i].size(); j++)
				{
					cout << pid[i][j].first<<"("<<pid[i][j].second<<")" << " ";
				}
				cout << endl;
			}

			*/
			

		}
	} while (FALSE);


	SQLFreeHandle(SQL_HANDLE_STMT, SQLStatementHandle);
	SQLDisconnect(SQLConnectionHandle);
	SQLFreeHandle(SQL_HANDLE_DBC, SQLConnectionHandle);
	SQLFreeHandle(SQL_HANDLE_ENV, SQLEnvHandle);
	// Frees the resources and disconnects

	//getchar();

	return 0;
}


